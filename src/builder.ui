<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object id="window" class="GtkWindow">
    <property name="title">Grid</property>
    <property name="default-height">700</property>
    <property name="default-width">900</property>
    <child>
      <object id="grid" class="GtkGrid">
      	<!-- GCODE BUTTONS -->
        <child>
          <object id="button0" class="GtkButton">
            <property name="label">Button 1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button1" class="GtkButton">
            <property name="label">Button 1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button2" class="GtkButton">
            <property name="label">Button 1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button3" class="GtkButton">
            <property name="label">Button 1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button4" class="GtkButton">
            <property name="label">Button 1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button5" class="GtkButton">
            <property name="label">Button 1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        
        <!-- Numeric inputs -->
        <child>
          <object id="spinner0" class="GtkSpinButton">
            <property name="adjustment">
              <object id="adjustment" class="GtkAdjustment">
                <property name="value">420.0</property>
                <property name="lower">69.0</property>
                <property name="upper">9001.0</property>
                <property name="step_increment">0.1</property>
                <property name="page_increment">1.0</property>
                <property name="page_size">0.0</property>
              </object>
            </property>
            <property name="digits">2</property>
            <property name="value">420.00</property>
            <layout>
              <property name="column">2</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>

        <!-- Exit Button -->
        <child>
          <object id="quit" class="GtkButton">
            <property name="label">Quit</property>
            <layout>
              <property name="column">0</property>
              <property name="row">6</property>
              <property name="column-span">3</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>